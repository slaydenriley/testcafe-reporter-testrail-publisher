{
  "name": "testcafe-reporter-testrail-submittable",
  "version": "1.1.0",
  "description": "testcafe-reporter-testrail-submittable TestCafe reporter plugin.",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slaydenriley/testcafe-reporter-testrail-publisher.git"
  },
  "author": "Riley Slayden",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "jest--init": "jest --init",
    "check-format": "prettier --list-different \"{bin,src}/**/*.ts\"",
    "format": "prettier --write \"{bin,src}/**/*.ts\"",
    "tsc": " tsc",
    "tsc:init": "tsc --init",
    "tslint:init": "tslint --init",
    "eslint-fix": "eslint ./ --ext .js,.ts,.tsx --fix",
    "build": "rimraf lib && tsc",
    "package": "npm run build && npm run rimraf:package && rm -f my*.tgz && npm pack",
    "rimraf:package": "rimraf ./package",
    "publish": "npm publish --access public"
  },
  "keywords": [
    "testcafe",
    "reporter",
    "plugin",
    "testrail"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/slaydenriley/testcafe-reporter-testrail-publisher/issues"
  },
  "homepage": "https://github.com/slaydenriley/testcafe-reporter-testrail-publisher#readme",
  "dependencies": {
    "@types/minimist": "1.2.0",
    "@types/node": "14.0.24",
    "@types/semver": "7.3.1",
    "ansi-to-html": "0.6.14",
    "callsite-record": "4.1.3",
    "chalk": "2.4.2",
    "handlebars": "4.7.7",
    "minimist": "1.2.5",
    "semver": "7.3.2",
    "testrail-api": "^1.3.6"
  },
  "devDependencies": {
    "@types/jest": "26.0.6",
    "@typescript-eslint/eslint-plugin": "3.7.0",
    "@typescript-eslint/parser": "3.7.0",
    "eslint": "7.5.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "26.1.0",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "testcafe": "1.9.0",
    "ts-jest": "26.1.3",
    "ts-node": "8.10.2",
    "typescript": "3.9.7"
  },
  "peerDependencies": {
    "testcafe": "1.x"
  }
}
